{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es2016",                                
    /* Modules */
    "module": "commonjs",                               
     "outDir": "./dist",                                  
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "strictPropertyInitialization": false,             
    "skipLibCheck": true                                 
  }
}
